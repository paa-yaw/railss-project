 <style>
 #region{
  padding-top: 50px;
 }
 </style>

 <% flash.each do |key, message| %>
    <p style="position:absolute; z-index:10000; margin:0 auto;left:0; right:0; width:40%; background:#BDE5F8; color:#00529B; padding:10px;" class="well well-sm text-center" id="flashmessage">
      <%= message %>
    </p>
 <% end %>

 <header id="header">
    <nav class="main-navigation navbar-fixed-top" role="navigation" data-spy="affix" data-offset-top="200">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            <i class="fa fa-bars" aria-hidden="true"></i>
          </button>
          <!-- logo here -->
          <a href="/" class="navbar-brand"><%= image_tag "logo.png", width:'100px' %></a>
        </div>
        <!-- Start Navigation Menu -->
        <div class="collapse navbar-collapse">
          <ul class="nav navbar-nav navbar-right" id="main_navigation_menu">
                <% if current_client %>
           <!--  <li>
              <li><a href="/articles">Blog</a></li>      
            </li> -->
            <li>
                <%= link_to "Your Requests", my_requests_path %>
                <script type="text/javascript">
                  mixpanel.track_forms("#new_request", "Complete Request Form");
                </script>
            </li>
            <li class="dropdown">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><%= current_client.first_name %><span class="caret"></span>
                </a>
                <ul class="dropdown-menu">
                    <li style="margin-left:1px;">
                        <%= link_to "Edit Profile", edit_client_registration_path %>
                    </li>
                    <li  style="margin-left:1px;">
                      <%= link_to "Referrals", referrals_path %>
                    </li>
                    <li  style="margin-left:1px;">
                      <%= link_to "Invite Others", new_referral_path %>
                    </li>
                    <li  style="margin-left:1px;">
                      <%= link_to "Log Out", destroy_client_session_path, method: :delete, data: {confirm: "#{current_client.email}, are you sure you want to log out?"} %>
                    </li> 
                <% else %>
                    <li>
                      <%= link_to "Log In", new_client_session_path %>
                    </li>
                </ul>
              </li>
                <% end %>
          </ul>
        </div>
        <!-- End Navigation Menu -->
      </div>
    </nav>
  </header>

 <div id="region" style="min-height:100%;">

   <%= yield %>
 </div>

<script>

setTimeout(fade_out, 5000);

function fade_out() {
 $("#flashmessage").fadeOut().empty();
}

</script>
<script>
    $(document).ready(function () {
        $('.dropdown-toggle').dropdown();
    });
</script>
